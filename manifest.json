{
    "name": "prompt-gtp",
    "version": "1.0",
    "manifest_version": 2,
    "description": "This extention display buttons on side of the search bar with prompts",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["https://chat.openai.com/*"],
        "js": ["content.js"],
        "css": ["style.css"]
      }
    ]
  }
  
